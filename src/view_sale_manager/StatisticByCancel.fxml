<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="500.0" prefWidth="829.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller_sale_manager.StatisticByCancelController">
   <top>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <Label prefHeight="30.0" text="Date:" />
                  <DatePicker fx:id="txtStartDate" prefHeight="30.0">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </HBox.margin>
                  </DatePicker>
                  <HBox alignment="CENTER">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </HBox>
                  <Label prefHeight="30.0" text="Date:" />
                  <DatePicker fx:id="txtEndDate" prefHeight="30.0" />
               </children>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </HBox>
            <Button fx:id="searchButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Search">
               <HBox.margin>
                  <Insets right="15.0" top="15.0" />
               </HBox.margin>
            </Button>
            <VBox>
               <children>
                  <Label prefHeight="30.0" text="Number of orders:" />
