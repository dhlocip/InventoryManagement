            </VBox>
            <VBox>
               <children>
                  <Label fx:id="numberBills" prefHeight="30.0" prefWidth="96.0" text="0" />
                  <Label fx:id="totalCancel" prefHeight="30.0" text="0" />
               </children>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </top>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <center>
      <HBox prefHeight="420.0" prefWidth="1112.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="cancelTable" prefHeight="420.0" prefWidth="1176.0">
               <columns>
                  <TableColumn fx:id="userID" prefWidth="75.0" text="UserID" />
                  <TableColumn fx:id="billId" prefWidth="75.0" text="Bill ID" />
                  <TableColumn fx:id="productId" prefWidth="75.0" text="Product ID" />
                  <TableColumn fx:id="price" prefWidth="76.0" text="Price" />
                  <TableColumn fx:id="quantity" prefWidth="81.0" text="Quantity" />
                  <TableColumn fx:id="total" prefWidth="61.0" text="Total" />
                  <TableColumn fx:id="transactionDate" prefWidth="150.0" text="TransactionDate" />
                  <TableColumn fx:id="paymentName" prefWidth="75.0" text="PaymentName" />
                  <TableColumn fx:id="statusCancel" prefWidth="75.0" text="StatusCancel" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
   </center>
</BorderPane>
